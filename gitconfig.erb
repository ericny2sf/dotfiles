[user]
	name = Javier Julio
	email = <%= prompt_for_value("email") %>
[core]
	excludesfile = ~/.gitignore
[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  # Easy access to show list of all aliases in this file.
  a = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
  ci = commit
  co = checkout
  f = fetch
  go = checkout -B
  rb = rebase
  s = status