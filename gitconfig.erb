[user]
	name = Javier Julio
	email = <%= prompt_for_value("email") %>
[core]
	excludesfile = ~/.gitignore
[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  # Easy access to show list of all aliases in this file.
  a = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
  ci = commit
  co = checkout
  f = fetch
  fo = fetch origin
  go = checkout -B
  pom = push origin master
  phm = push heroku master
  r = rebase
  rb = rebase
  s = status
  # Undo the last commit but keep changes intact.
  uci = reset --soft HEAD^